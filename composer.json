{
  "name": "orchid/experiment",
  "description": "AB tests for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandr Chernyaev",
      "email": "bliz48rus@gmail.com",
      "homepage": "https://tabuna.github.io",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Orchid\\Experiment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Orchid\\Experiment\\Tests\\": "tests/"
    }
  },
  "require": {
    "ext-json": "*",
    "laravel/framework": "^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "~7.0",
    "phpunit/php-code-coverage": "^9.0",
    "phpunit/phpunit": "^9.0"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Orchid\\Experiment\\ExperimentServiceProvider"
      ]
    }
  }
}
